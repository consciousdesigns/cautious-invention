#!/bin/bash
# Setup script for Conscious-Designs3

echo "Setting up development environment..."

# Install Python
sudo apt-get update
sudo apt-get install -y python3 python3-pip

# Install MySQL Server
sudo apt-get install -y mysql-server

echo "Setup complete!"
# Python
__pycache__/
*.pyc

# Node
node_modules/

# VS Code
.vscode/

# OS files
.DS_Store

# Others
.env
